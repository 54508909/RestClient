<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liao.mapper.HotelMapper">

    <select id="getHotel" resultType="com.liao.pojo.Hotel">
        SELECT
	*
FROM
	tb_hotel
WHERE
	id = 1
    </select>
    <select id="getHotelList" resultType="com.liao.pojo.Hotel">
        SELECT
	*
FROM
	tb_hotel
    </select>
</mapper>